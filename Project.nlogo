;=================================================================================
;    Part 4: Validation Functions
;=================================================================================
; These functions can be run to validate the operation of the functions. The
; inputs and outputs are provided so that the results can be compared with
; hand calculations.

; Validation of bidding process functions:
; MP-Select-Bid, RC-Prepare-Bid, MP-Pick-Winners
to setup-bid-validation
  ; Create an empty space for the model to run in
  clear-all
  reset-ticks

  ; Give values to the global variables
  ;------------------- Recycling company related --------------
  set G_tech_capacity_avg 5000                                             ;; slider
  set G_tech_sep_eff_avg 0.85                                              ;; slider 0-100%

  ; Create the households and municipalities
  create-MPs 2 [
    set MP_total_waste 20000
    set MP_plastic_waste 4000
    set MP_recyclable_waste 3000
  ]

  ; Create the recycling companies
  create-RCs 10 [
    let capacity round G_tech_capacity_avg * (1 + (random 50 - 25) / 100)
    let sep_eff min list (G_tech_sep_eff_avg * (1 + (random 10 - 5) / 100)) 1
    let tech (list capacity sep_eff 0 0)
    set RC_list_of_techs (list tech)
  ]

  ask MPs [

    let bids []
    ask RCs [
      let bid RC-Prepare-Bid
      set bids lput (bid) bids
    ]

    output-print (word "Municipality: " self)
    output-print (word "Waste generation rate (/month): " MP_plastic_waste)
    output-print "-------------------------"
    output-print "Bids presented by Recycling Companies"
    output-print (word "RC-own/capacity/sep eff/fee/fine")
    foreach bids [ [?1] ->
      output-print (word item 1 ?1 "    " round item 2 ?1 "    " (round (item 3 ?1 * 100) / 100) "    " item 4 ?1 "    " item 5 ?1)
    ]
    output-print "-------------------------"
    MP-Select-Bid
    output-print "Winning Bids:"
    output-print "RC-own/Capacity/Overall Rank"
    ask MP_my_contracts [output-print (word CT_RC_own "  " CT_capacity "  " CT_overallrank)]
    output-print ""
  ]

end

; Validation of waste generation and collection functions:
; HH-Gather-Waste and MP-Collect-Waste
to setup-waste-validation

  ; Create an empty space for the model to run in
  clear-all
  reset-ticks

  ; Give values to the global variables
  ;----------------------- Household related ------------------
  set G_initial_perception_avg 1                                          ;; slider 0-100%
  set G_initial_knowledge_avg 0.5                                          ;; slider 0-100%
  set G_household_total_plastic_pct 1                                  ;; slider 0-100%
  set G_household_total_recyclable_pct 0.5                                ;; slider 0-100%
  set G_occupancy_qty_list (list 4 2 1 2)                                 ;;
  ;---------------------- Municipality related ----------------
  set G_pct_budget_spending 1                                             ;; slider 0-100%
